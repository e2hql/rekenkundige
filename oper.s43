;----------------------------------------------------------------------------------------------
#include "msp430g2553.h"
;----------------------------------------------------------------------------------------------
        NAME   OPER
        PUBLIC SOMA_OPER, SUB_OPER, TESTE
        RSEG   CODE   
;----------------------------------------------------------------------------------------------
SOMA_OPER
;----------------------------------------------------------------------------------------------          
        push.w    R4
        push.w    R5
        add.w     R4,R5
        mov.w     R5,R6
        pop.w     R4
        pop.w     R5
        ret
;----------------------------------------------------------------------------------------------
SUB_OPER
;----------------------------------------------------------------------------------------------         
        push.w   R4
        push.w   R5
        mov.w    R5,R6
        pop.w    R4
        pop.w    R5
        ret
;----------------------------------------------------------------------------------------------
LSB
;----------------------------------------------------------------------------------------------
        mov.b      R4,R5
        nop     
        ret 
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
MSB
;----------------------------------------------------------------------------------------------
        rra         R4
        rra         R4
        rra         R4
        rra         R4
        rra         R4
        rra         R4
        rra         R4
        rra         R4
        mov.b       R4,R4
        ret 
;----------------------------------------------------------------------------------------------
;----------------------------------------------------------------------------------------------
        END  
;----------------------------------------------------------------------------------------------
